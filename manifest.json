{
  "manifest_version": 2,

  "name": "facext",
  "description": "face detection",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "facext.html"
  },

  "options_page": "options.html",

  "permissions": [
    "activeTab"
  ],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
